{
    "aria2c": {
        "rpc-listen-port": 6800,
        "dir": "{HOME}/Downloads/aria2",
        "log": "{HOME}/.aria2/aria2.log",
        "log-level": "error",
        "dht-listen-port": 6801,
        "min-split-size": "1M",
        "max-connection-per-server": 4,
        "save-session": "{HOME}/.aria2/session",
        "input-file": "{HOME}/.aria2/session",
        "rpc-listen-all": "false"
    },
    "berserker": {
        "aria2c_executable": "aria2c",
        "connector": "websocket",
        "server_port": 8000
    }
}